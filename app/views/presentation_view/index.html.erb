
<!-- 発表資料閲覧画面 -->
<h1>発表資料閲覧ページです</h1>
<h2>閲覧したい発表資料の公開日を指定してください。</h2>
<label for='pbltype'>プロジェクト種別</label>
<%= form_tag({:action => 'view'},{:remote => true}) do %>
<%= select_tag('pbltype',raw('<option value="PBL">PBL</option><option value="研究開発">研究開発</option>')) %>
<label for='publishdate'>公開日</label>
<%= select_date %>
<%= submit_tag '表示' %>
<% end %>
